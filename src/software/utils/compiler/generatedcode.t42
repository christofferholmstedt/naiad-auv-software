procedure wow(iPtr : access(integer); n : float) is 
begin
	iPtr := integer(float(iPtr) * n);
end wow;

procedure pow(iPtr : access(integer); n : float) is 
begin
	wow(access(iPtr), n);
end pow;

procedure main is 
	myInt : integer;
begin
	myInt := 7;
	pow(access(myInt), error );
	asm(PUSHINT -1);
	asm(PUSHFP);
	asm(ADDINT);
	asm(RVALINT);
	asm(PRINTINT);
	asm(POPINT);
end main;

